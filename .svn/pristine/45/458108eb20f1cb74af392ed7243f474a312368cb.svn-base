<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>dataParsingserver</display-name>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>classpath*:applicationContext*.xml</param-value>
  </context-param>
  <context-param>
    <param-name>webAppRootKey</param-name>
    <param-value>ybt.root</param-value>
  </context-param>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>springServlet</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <init-param>
      <param-name>contextConfigLocation</param-name>
      <param-value>classpath*:/spring-mvc.xml</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>springServlet</servlet-name>
    <url-pattern>*.htm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InitSystemServlet</servlet-name>
    <servlet-class>com.ybt.web.init.InitSystemServlet</servlet-class>
    <load-on-startup>3</load-on-startup>
  </servlet>
  <filter>
    <filter-name>encodingFilter</filter-name>
    <filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>encodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>jpaFilter</filter-name>
    <filter-class>org.springframework.orm.jpa.support.OpenEntityManagerInViewFilter</filter-class>
    <init-param>
      <param-name>singleSession</param-name>
      <param-value>false</param-value>
    </init-param>
    <init-param>
      <param-name>entityManagerFactory</param-name>
      <param-value>entityManagerFactory</param-value>
    </init-param>
    <init-param>
      <param-name>excludeSuffixs</param-name>
      <param-value>js,css,jpg,jpeg,gif,png,bmp,html</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>jpaFilter</filter-name>
    <url-pattern>*.htm</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>sitemesh</filter-name>
    <filter-class>org.sitemesh.config.ConfigurableSiteMeshFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>sitemesh</filter-name>
    <url-pattern>*.htm</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>auth</filter-name>
    <filter-class>com.ybt.web.filter.AuthFilter</filter-class>
    <init-param>
      <param-name>LOGIN_URL</param-name>
      <param-value>/login.htm</param-value>
    </init-param>
    <init-param>
      <param-name>REGIST_URL</param-name>
      <param-value>/register.htm</param-value>
    </init-param>
    <init-param>
      <param-name>CODE_URL</param-name>
      <param-value>/code.htm</param-value>
    </init-param>
    <init-param>
      <param-name>LOGINING_URL</param-name>
      <param-value>/loging.htm</param-value>
    </init-param>
    <init-param>
      <param-name>REGISTING_URL</param-name>
      <param-value>/registing.htm</param-value>
    </init-param>
    <init-param>
      <param-name>NOAUTH_URL</param-name>
      <param-value>/noauth.htm</param-value>
    </init-param>
    <init-param>
      <param-name>MESSAGE_URL</param-name>
      <param-value>/message.htm</param-value>
    </init-param>
    <init-param>
      <param-name>REAPLY_URL</param-name>
      <param-value>/finance/reApply.htm</param-value>
    </init-param>
    <init-param>
      <param-name>MYAPLY_URL</param-name>
      <param-value>/finance/myapply.htm</param-value>
    </init-param>
    <init-param>
      <param-name>APP_URL</param-name>
      <param-value>/appDownload.htm</param-value>
    </init-param>
    <init-param>
      <param-name>QUES_URL</param-name>
      <param-value>/personal/question.htm</param-value>
    </init-param>
    <init-param>
      <param-name>CGPS_URL</param-name>
      <param-value>/personal/changeLoginPassword.htm</param-value>
    </init-param>
    <init-param>
      <param-name>SP_URL</param-name>
      <param-value>/personal/saveChangeLgPassword.htm</param-value>
    </init-param>
    <init-param>
      <param-name>INIPASS_URL</param-name>
      <param-value>/personal/initChangePassword.htm</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>auth</filter-name>
    <url-pattern>*.htm</url-pattern>
  </filter-mapping>
  <servlet>
    <description>Direct Web Remoter Servlet</description>
    <servlet-name>dwr-invoker</servlet-name>
    <servlet-class>org.directwebremoting.servlet.DwrServlet</servlet-class>
    <init-param>
      <param-name>config</param-name>
      <param-value>/WEB-INF/classes/dwr.xml</param-value>
    </init-param>
    <init-param>
      <param-name>pollAndCometEnabled</param-name>
      <param-value>true</param-value>
    </init-param>
    <init-param>
      <param-name>debug</param-name>
      <param-value>false</param-value>
    </init-param>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>dwr-invoker</servlet-name>
    <url-pattern>/dwr/*</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/index.htm</welcome-file>
  </welcome-file-list>
</web-app>